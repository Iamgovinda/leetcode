# https://leetcode.com/problems/two-sum/

class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        items_map = dict()
        for index, num in enumerate(nums):
            x = target - num
            if x in items_map:
                return [items_map[x], index]
            items_map[num]=index
        
        
